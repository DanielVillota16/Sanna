<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProviderHome">

    <Button
        android:id="@+id/LogOutProviderBtn"
        style="@style/SecondButton"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="33dp"
        android:text="Log Out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ToInventoryBtn"
        style="@style/SecondButton"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_marginTop="156dp"
        android:text="Inventario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ToProviderOrderBtn"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ToProviderOrderBtn"
        style="@style/SecondButton"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_marginTop="156dp"
        android:text="Ordenes"
        app:layout_constraintEnd_toStartOf="@+id/ToInventoryBtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>